<p id="notice"><%= notice %></p>

<h1>Listing Teacher Matters</h1>

<table>
  <thead>
    <tr>
      <th>Teacher</th>
      <th>Matter</th>
      <th>Custo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @teacher_matters.each do |teacher_matter| %>
      <tr>
        <td><%= teacher_matter.teacher_id %></td>
        <td><%= teacher_matter.matter_id %></td>
        <td><%= teacher_matter.custo %></td>
        <td><%= link_to 'Show', teacher_matter %></td>
        <td><%= link_to 'Edit', edit_teacher_matter_path(teacher_matter) %></td>
        <td><%= link_to 'Destroy', teacher_matter, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Teacher matter', new_teacher_matter_path %>
