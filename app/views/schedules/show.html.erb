<p id="notice">
	<%= notice %>
</p>

<p>
	<strong>Type:</strong>
	<%= @schedule.schedule_type %>
</p>

<p>
	<strong>Start time:</strong>
	<%= @schedule.start_time %>
</p>

<p>
	<strong>Schedule Father:</strong>
	<%= @schedule.schedule_father.ident unless @schedule.schedule_father.nil? %>
</p>
<p>
	<strong>Teaching:</strong>
	<%= @schedule.schedule_teaching.ident unless @schedule.schedule_teaching.nil? %>
</p>

<p>
	<%= h_show_label(:label_schedule_presents)%>
	<%= h_table(@schedule.presents,[:present_type,:student__lnk])%>
</p>

<p>
	<%= h_show_label(:label_schedule_schedules)%>
	<%= h_table(@schedule.schedules,[:schedule_type,:start_time_date,:schedule_father__lnk,:schedule_teaching__lnk])%>

</p>

<<%= h_show_bottom(@schedule)%>
<%= h_show_menus_bottom(@schedule)%>

